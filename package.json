{
  "name": "@waiter/factory",
  "version": "2.2.0-alpha.1",
  "description": "Waiter Factory Library for Javascript",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "docs",
    "example",
    "lib"
  ],
  "directories": {
    "doc": "./docs",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./docs ./coverage ./.nyc_output",
    "doc": "echo \"implementing...\" && exit 0",
    "mocha": "mocha",
    "nyc": "nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "npm run nyc && nyc report --reporter=text-lcov | coveralls",
    "snyk": "snyk wizard",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "prepare": "npm run clean && npm run build",
    "preversion": "npm run prepare && npm test && npm run doc",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm test && npm run doc"
  },
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "name": "Tetsu Yamazaki",
      "email": "yamazaki@motionpicture.jp"
    }
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/waiter-jp/factory.git"
  },
  "dependencies": {
    "setprototypeof": "^1.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^9.6.39",
    "@types/power-assert": "^1.5.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.2",
    "snyk": "^1.111.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  }
}
